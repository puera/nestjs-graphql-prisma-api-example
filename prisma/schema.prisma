generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Donations {
  id          Int      @id @default(autoincrement())
  count       Int
  displayName String
  email       String
  mobile      String?
  message     String?
  team        String?
  createdAt   DateTime @default(now())

  @@map("donations")
}
